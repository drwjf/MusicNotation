
source 'https://github.com/artsy/Specs.git'
source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!

inhibit_all_warnings!

target 'MusicNotationLibrary' do
    platform :osx, '10.11'
    pod 'CoreParse'
    #    pod 'AFNetworking'
    #    pod 'ReactiveCocoa'
    pod 'CocoaLumberjack'
    pod 'ReflectableEnum'
    pod 'RegExCategories'
    pod 'pop'
    pod 'TheAmazingAudioEngine'
    link_with 'MusicNotationLibrary'
end

target 'MusicNotationMobile' do
    platform :ios, '9.1'
    pod 'CoreParse'
    #    pod 'AFNetworking'
    #    pod 'ReactiveCocoa'
    pod 'CocoaLumberjack'
    pod 'ReflectableEnum'
    pod 'RegExCategories'
    pod 'TheAmazingAudioEngine'
    link_with 'MusicNotationMobile'
end



